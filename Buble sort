
#include<stdio.h>
    void bubblesort(int arr[20],int n){
        int i,j;
        for(i=0;i<(n-1);i++){
            for(j=0;j<(n-i-1);j++){
                if(arr[i]>arr[j+1]){
                    int temp=0;
                    temp=arr[j];
                    arr[j]=arr[j+1];
                    arr[j+1]=temp;
                }
            }
        }
}

void printArray(int arr[], int n)
{
    int i;
    for (i = 0; i < n; i++)
        printf("%d ", arr[i]);
}

int main(){

    int arr[20] ;
    int n=5,i;
    int searchElement ;

    printf("Enter array elements:\n");
    for( i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    bubblesort( arr, n);
    printf("After sorting the elements are :%d",arr[i]);
    printArray( arr,  n);
    return 0;
}
